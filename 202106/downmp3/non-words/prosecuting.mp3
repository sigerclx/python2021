                                }
                                if (settings.type == 'ul')  {
                                    inputs(form).parent().find('ul.errorlist').remove();
                                    form.find('li:has(ul.errorlist)').remove();
                                    var has_one_field = false;
                                    $.each(data.errors, function(key, val)  {
                                        if (key.indexOf('__all__') >= 0)   {
                                            get_form_error_position(key).before('<li><ul class="errorlist"><li>' + val + '</li></ul></li>');
                                        }
                                        else {
                                            if (self.attr('id') == key) {
                                                $('#' + key).prev().before('<ul class="errorlist"><li>' + val + '</li></ul>');
                                                $('#' + key).parent().prevAll('li').find('ul.errorlist').show();
                                                has_one_field = true;
                                            } else {
                                                $('#' + key).prev().before('<ul class="hide errorlist"><li>' + val + '</li></ul>');
                                            }
                                        }
                                    });
                                    if (!has_one_field) {
                                        $('#' + self.attr('id')).parent().prevAll('li').find('ul.errorlist').show();
                                    };
                                }
                            }
                        } else {
                            form.find('ul.errorlist').remove();
                        }
                    },
                    type: 'POST',
                    url: url
                });
                if (status && settings.submitHandler) {
                    return settings.submitHandler.apply(this);
                }
                return status;
            });
        });
    };
})(jQuery);
</script>

    
